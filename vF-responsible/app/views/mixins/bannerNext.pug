mixin bannerNext(txt)
	- var nbrBannerPct =  100 / txt.length  + "%";
	- var nbrBannerPx =   650 /  txt.length;
	- console.log(nbrBanner, 'nbrBanner') 
	tr
		td(width='650', align='center', style='border-collapse: collapse; mso-line-height-rule: exactly; max-width:650px; width:100%;')
			table(width='650', cellpadding='0', cellspacing='0', border='0', style='background: white; border-collapse: collapse; mso-table-lspace: 0pt; mso-table-rspace: 0pt; max-width:650px; width:100%;')
				tr
					each bannerDouble in txt
						td(width= nbrBannerPx , style="width:" + nbrBannerPct + "; max-width:" + nbrBannerPx + "px;")
							a(href!= BaseUrl + bannerDouble.link + meta, style='text-decoration: none;')
								img.full--img(width= nbrBannerPx , src= bannerDouble.src, alt= bannerDouble.alt, style='-ms-interpolation-mode: bicubic; border: 0 none; display: block; height: auto; line-height: 100%; max-width: 100% !important; outline: none; text-decoration: none; width: 100% !important;')